                        ]
                    },
                    "pages": {
                        "tabField": {
                            "ui": [
                                [
                                    {
                                        "metadata": "items,,24,,aiDialogEditor",
                                        "optionJsx.config": {
                                            "form": "FormCI",
                                            "dialog": "选择配置项,保存,关闭,false,900,true,$opSaveCi",
                                            "op": {
                                                "add": "$opShowCi"
                                            },
                                            "table": {
                                                "columns": [
                                                    {
                                                        "title": "操作",
                                                        "dataIndex": "key",
                                                        "fixed": "left",
