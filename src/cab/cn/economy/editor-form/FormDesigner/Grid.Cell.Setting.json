                            "tabCascade": {
                                "window": 1,
                                "columns": 4,
                                "ui": [
                                    [
                                        "title=级联/依赖属性"
                                    ],
                                    [
                                        "dependImpact,触发重置,24,,aiTextArea,placeholder=填写多个字段名，字段名之间使用英文逗号隔开。,rows=3"
                                    ],
                                    [
                                        "dependEnable,依赖字段,16,,,placeholder=当前字段在依赖字段取值时启用/禁用"
                                    ],
                                    [
                                        {
                                            "metadata": "dependType,依赖字段类型,16,,aiRadio",
                                            "optionJsx.config.items": [
                                                "PURE,纯值",
                                                "DATUM,动态数据源"
                                            ]
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "dependSource,数据源标识,12,,aiSelect,placeholder=辅助数据源中配置",
                                            "optionJsx.config.datum": "source=model.sources,key=key,label=name"
                                        },
                                        "dependField,条件字段,12"
                                    ],
                                    [
                                        {
                                            "metadata": "dependValue,条件字段值,24,,aiInputMulti",
                                            "optionJsx.styleInput": {
                                                "width": "23%"
                                            }
                                        }
                                    ]
                                ]
                            },
                            "tabSelector": {
                                "window": 1,
                                "columns": 3,
                                "ui": [
                                    [
                                        "title=选项属性"
                                    ],
                                    [
                                        {
                                            "metadata": "dataSource,数据源,16,,aiRadio",
                                            "optionJsx.config.items": [
                                                "items,静态数据源",
                                                "datum,动态数据源"
                                            ]
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "datumSource,数据标识,,,aiSelect,placeholder=辅助数据源中配置",
                                            "optionJsx.config.datum": "source=model.sources,key=key,label=name"
                                        },
                                        "datumKey,值字段名",
                                        "datumLabel,显示字段名"
                                    ],
                                    [
                                        "expr,格式化表达式,24,,,placeholder=显示格式可使用格式化表达式进行格式化操作！"
                                    ],
                                    [
                                        {
                                            "metadata": "cascadeEnabled,启用过滤,,,aiCheckbox",
                                            "optionJsx": {
                                                "mode": "SWITCH",
                                                "checkedChildren": "启用过滤",
                                                "unCheckedChildren": "不启用过滤"
                                            },
                                            "optionConfig": {
                                                "valuePropName": "checked"
                                            }
                                        },
                                        "cascadeSource,过滤字段名,16,,,placeholder=请填写使用的动态数据源中过滤条件字段"
                                    ],
                                    [
                                        {
                                            "metadata": "cascadeTarget,过滤参数类型,24,,aiRadio",
                                            "optionItem.className": "radio-param-selection",
                                            "optionJsx.config.items": [
                                                "BOOL,逻辑值",
                                                "FIX,固定值",
                                                "ENUM,枚举值",
                                                "UNIQUE,字典值",
                                                "USER,当前登录用户",
                                                "FORM,表单值",
                                                "ROUTE,路由值",
                                                "PROP,属性值（React组件）",
                                                "STATE,状态值（React组件）"
                                            ]
                                        }
                                    ],
                                    [
                                        "cascadeValue,过滤参数值,24"
                                    ],
                                    [
                                        {
                                            "metadata": "items,选项表,24,,aiTableEditor",
                                            "optionJsx.config": {
                                                "format": {
                                                    "type": "ARRAY",
                                                    "keyField": "name"
                                                },
                                                "table": {
                                                    "columns": [
                                                        {
                                                            "dataIndex": "name",
                                                            "title": "固定值",
                                                            "$render": "EDITOR"
                                                        },
                                                        {
                                                            "dataIndex": "label",
                                                            "title": "显示字段",
                                                            "$render": "EDITOR"
                                                        }
                                                    ]
                                                }
                                            }
                                        }
                                    ]
                                ]
                            },
                            "tabSepc": {
                                "window": 1,
                                "columns": 3,
                                "ui": [
                                    [
                                        "title=数值属性"
                                    ],
                                    [
                                        "min,最小值,,68%,aiInputNumber",
                                        "precision,精度,,68%,aiInputNumber,min=1",
                                        "step,步进系数,,68%,aiInputNumber,placeholder=默认为1"
                                    ],
                                    [
                                        "max,最大值,,68%,aiInputNumber",
                                        {
                                            "metadata": "iNumberFormat,特殊格式,,,aiRadio",
                                            "optionJsx.config.items": [
                                                "PERCENT,百分比",
