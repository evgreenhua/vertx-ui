                                    [
                                        {
                                            "metadata": "treeSelection,树选择模式,24,,aiRadio",
                                            "optionJsx.className": "radio-tree-selection",
                                            "optionJsx.config.items": [
                                                "PARENT_ALL_INCLUDE,含当前所有父节点",
