                                                "unCheckedChildren": "（标准格式）显示"
                                            },
                                            "optionConfig": {
                                                "valuePropName": "checked"
                                            }
                                        }
                                    ],
                                    [
                                        "title=上传/下载属性"
                                    ],
                                    [
                                        {
                                            "metadata": "listType,上传模式,16,,aiRadio",
                                            "optionJsx.config.items": [
                                                "text,文件模式",
                                                "picture,图片模式",
                                                "picture-card,卡片图模式"
                                            ]
                                        },
                                        {
                                            "metadata": "withCredentials,携带Cookie？,,,aiCheckbox",
                                            "optionJsx": {
                                                "mode": "SWITCH",
                                                "checkedChildren": "携带Cookie上传",
                                                "unCheckedChildren": "不携带Cookie上传"
                                            },
                                            "optionConfig": {
                                                "valuePropName": "checked"
                                            }
                                        }
                                    ],
                                    [
                                        "fileText,显示文字",
                                        "accept,支持格式,16,,,placeholder=使用MIME，用逗号隔开"
