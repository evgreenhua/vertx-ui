                    "metadata": "readOnly,只读,,,aiCheckbox",
                    "optionJsx": {
                        "mode": "SWITCH",
                        "checkedChildren": "只读",
                        "unCheckedChildren": "可编辑"
                    },
                    "optionConfig": {
                        "valuePropName": "checked"
                    }
                }
            ],
            [
                {
                    "metadata": "normalize,输入限制,16,,aiRadio",
                    "optionJsx.config.items": [
                        "decimal,仅浮点数",
                        "number,仅数值",
                        "integer,仅正整数",
                        "upper,仅大写",
                        "id,仅标识符"
                    ]
                },
                "inscribe,只读文本,,,,placeholder=只读模式下的呈现文本"
            ],
            [
                {
                    "complex": true,
                    "name": "FieldContainer",
                    "key": "advancedSetting",
                    "config": {
                        "activeKey": "tabIntro",
                        "defaultActiveKey": "tabIntro",
                        "className": "tool-setting",
