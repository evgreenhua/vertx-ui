{
    "_form": {
        "window": 1,
        "columns": 3,
        "className": "setting-form-cell",
        "ui": [
            [
                "title=字段基础属性"
            ],
            [
                "render,组件类型",
                "label,标签/标题,,,,placeholder=<标签>",
                {
                    "metadata": "field,字段名,,,aiSelect",
                    "optionJsx.config.datum": "source=model.fields,key=key,label=display"
                }
            ],
            [
                {
                    "metadata": "visibilityToggle,显示密码,,,aiCheckbox",
                    "optionJsx": {
                        "mode": "SWITCH",
                        "checkedChildren": "可显示",
                        "unCheckedChildren": "不可显示（密码框专用）"
                    },
                    "optionConfig": {
                        "valuePropName": "checked"
                    }
                },
                "addonBefore,组件前缀,,,,addonBefore=…",
                "addonAfter,组件后缀,,,,addonAfter=…"
            ],
            [
                {
                    "metadata": "allowClear,允许清空,,,aiCheckbox",
                    "optionJsx": {
                        "mode": "SWITCH",
                        "checkedChildren": "启用",
                        "unCheckedChildren": "禁用"
                    },
                    "optionConfig": {
                        "valuePropName": "checked"
                    }
                },
                {
                    "metadata": "prefix,文字前缀",
                    "optionJsx.prefix": "…"
                },
                {
                    "metadata": "suffix,文字后缀",
                    "optionJsx.suffix": "…"
                }
            ],
            [
                "maxLength,最大长度,,68%,aiInputNumber,placeholder=默认256,min=1,max=4096",
                "rows,最大行数,,68%,aiInputNumber,placeholder=多行文本专用,min=2,max=10",
