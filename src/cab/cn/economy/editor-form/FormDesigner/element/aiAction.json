{
    "_form": {
        "window": 1,
        "columns": 2,
        "className": "setting-form-cell",
        "ui": [
            [
                "title=属性设置器"
            ],
            [
                {
                    "metadata": "render,组件类型,20",
                    "optionItem": {
                        "style": {
                            "width": "100%"
                        },
                        "labelCol": {
                            "span": 4
                        },
                        "wrapperCol": {
                            "span": 20
                        }
                    }
                },
                {
                    "metadata": "$button",
                    "optionItem": {
                        "style": {
                            "float": "right"
                        }
                    },
                    "optionJsx.extension": [
                        "$opSaveSetting,保存,SUBMIT,,ux-success-setting,save"
                    ],
                    "span": 4
                }
            ],
            [
                {
                    "complex": true,
                    "name": "FieldContainer",
                    "key": "fieldBasic",
                    "config": {
                        "activeKey": "tabSetting",
                        "defaultActiveKey": "tabSetting",
                        "className": "tool-setting",
                        "tabPosition": "right",
                        "items": "属性配置表,tabSetting",
                        "pages": {
                            "tabSetting": {
                                "window": 0.51,
                                "columns": 2,
                                "initial": {
                                    "field": "FIX:$button"
                                },
                                "ui": [
                                    [
                                        "title=常用属性"
                                    ],
                                    [
                                        "label,标签/标题,,,,placeholder=<标题>",
                                        {
                                            "metadata": "field,字段名,,,aiSelect,readOnly=true,placeholder=$button",
                                            "optionJsx.config.datum": "source=model.fields,key=key,label=display"
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "isHidden,是否隐藏,8,,aiCheckbox",
                                            "optionJsx": {
                                                "mode": "SWITCH",
                                                "checkedChildren": "隐藏按钮",
                                                "unCheckedChildren": "显示按钮"
                                            },
                                            "optionConfig": {
                                                "valuePropName": "checked"
                                            }
                                        },
                                        {
                                            "metadata": "actionMode,提交模式,16,,aiRadio",
                                            "optionJsx.config.items": [
                                                "CLIENT,客户端模式",
                                                "SERVER,服务端模式"
                                            ]
                                        }
                                    ],
                                    [
                                        "title=纯前端配置"
                                    ],
                                    [
                                        {
                                            "metadata": "clientId,按钮ID,,,,addonBefore=$",
                                            "optionJsx.depend.enabled": {
                                                "actionMode": [
                                                    "CLIENT"
                                                ]
                                            }
                                        },
                                        {
                                            "metadata": "clientText,按钮文字",
                                            "optionJsx.depend.enabled": {
                                                "actionMode": [
                                                    "CLIENT"
                                                ]
                                            }
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "clientType,按钮类型,24,,aiRadio",
                                            "optionJsx.depend.enabled": {
                                                "actionMode": [
                                                    "CLIENT"
                                                ]
                                            },
                                            "optionJsx.config.items": [
                                                "DEFAULT,默认按钮",
                                                "PRIMARY,标准按钮",
                                                "DANGER,提醒按钮"
                                            ]
                                        }
                                    ],
                                    [
                                        {
                                            "metadata": "clientColor,按钮风格,16,,aiRadio",
                                            "optionJsx.depend.enabled": {
                                                "actionMode": [
                                                    "CLIENT"
                                                ],
                                                "clientType": [
                                                    "DEFAULT"
                                                ]
                                            },
                                            "optionJsx.config.items": [
                                                "RED,红",
                                                "GREEN,绿",
                                                "SPEC,桃红",
                                                "BROWN,棕色"
                                            ]
                                        },
                                        {
                                            "metadata": "isReset,开启重置,8,,aiCheckbox",
                                            "optionJsx": {
                                                "mode": "SWITCH",
                                                "checkedChildren": "显示重置",
                                                "unCheckedChildren": "隐藏重置"
                                            },
                                            "optionJsx.depend.enabled": {
                                                "actionMode": [
                                                    "CLIENT"
                                                ]
                                            },
                                            "optionConfig": {
                                                "valuePropName": "checked"
                                            }
                                        }
                                    ]
                                ]
                            }
                        }
                    }
                }
            ]
        ]
    }
}